    <aura:attribute name="warnings" type="List"/>
    
    WARNINGS <br/>
     <aura:iteration items="{!v.warnings}" var="item">
         {!item.Code} and {!item.Description} <br/>
    </aura:iteration>
   
    
</aura:component>