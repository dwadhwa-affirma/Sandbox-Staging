}
.but-area .btn-primary {
	background: #0077bd;
	border-color: #337ab7;
	padding: 10px 12px !important;
	color: #fff;
	font-size: 12px;
	margin-right: 15px;
}

.but-area .btn-danger {
	background: #d9534f;
	border-color: #d43f3a;
	padding: 10px 12px !important;
	color: #fff;
	font-size: 12px;
	margin-right: 15px;
}

.but-area .btn-default {
	padding: 10px 12px !important;
}
.but-area .btn-default:hover {
	background-position: 0 0;
}
.but-area .btn-primary[disabled]{
	background:#b2b2b2;
	border-color:#b2b2b2;
	color:#666666;
	}
.btn-primary[disabled]:hover, .btn-primary[disabled]:focus{
	background:#b2b2b2 !important;
	border-color:#b2b2b2 !important;
	color:#666666;
	}
