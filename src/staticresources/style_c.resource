.but-area {
	border-top: 1px solid #ccc;
	float: left;
	width: 100%;
	position: relative;
	padding: 0;
	bottom: 0;
	left: 0;
	background: #fff;
}
.but-area input, .but-area button {
	margin: 15px 0 0;
}
