.chervon {
	position: relative;
	width: 100%;
	margin: 0 auto;
	float: left;
	background: #fff;
}
.chervon h2 {
	font-size: 22px;
	padding: 10px 15px 8px;
	margin: 0;
	float: left;
	width: 100%;
}
.chervon h2 b{
	font-family: 'Open Sans', sans-serif;
	font-weight:500;
	}
.seperator {
	border-bottom: 5px solid #ffcc00;
	width: 100%;
	margin: 5px 0 0px;
	float: left;
}
.seperator-confirm {
	border-bottom: 5px solid #00cc66;
	width: 100%;
	margin: 5px 0 0px;
	float: left;
}
.seperator-error {
	border-bottom: 5px solid #FF0000;
	width: 100%;
	margin: 5px 0 0px;
	float: left;
}
.wrapper {
	background: #e5e5e5 none repeat scroll 0 0;
	float: left;
	position: relative;
	width: 100%;
}
.left-panel {
	float: left;
	padding: 5px 15px 5px 15px;
	width: 100%;
}
.white-bg {
	float: right;
	background: #fff;
	padding: 5px 0px 5px 15px;
}
.left-panel h4, .white-bg h4, .chervon h4 {
	font-size: 18px;
	color: #0076bd;
	margin: 0;
	padding: 10px 0;
	float: left;
	width: 100%;
}
.form-control {
	box-shadow: none !important;
}
.confirm {
	color: #00cc66 !important;
	font-size: 12px;
	font-weight: normal;
	font-family: Gotham, "Helvetica Neue", Helvetica, Arial, sans-serif;
	left: -18px !important;
	top:7px;
}
.error {
	color: #ff0000 !important;
	font-size: 12px;
	font-weight: normal;
	left: -21px !important;
	top:5px;
	font-family: Gotham, "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.errorForTable {
	color: #ff0000 !important;
	font-size: 12px;
	font-weight: normal;
	left: -21px !important;
	top:5px;
	font-family: Gotham, "Helvetica Neue", Helvetica, Arial, sans-serif;
}


.error i {
