<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BrandImage__c</fullName>
    <description>Brand image. This formula uses the hardcoded url of the logo images in Production. This allows us to automate the sandbox refresh process.</description>
    <externalId>false</externalId>
    <formula>IF(Reference__c == &apos;BC02&apos;,     
     IMAGE(&quot;/resource/Logo_SpectrumCU&quot;,&quot;SpectrumCU&quot;, 35, 35 ),
     IMAGE(&quot;/resource/Logo_ChevronFCU&quot;,&quot;CFCU&quot;, 35, 35 )
)</formula>
    <label>Brand</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
