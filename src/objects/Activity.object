    <fields>
        <fullName>Closed_Date__c</fullName>
        <externalId>false</externalId>
        <label>Closed Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Created_By_Assigned_To_Me__c</fullName>
        <externalId>false</externalId>
        <formula>OR(
  $User.Id  =  CreatedBy.Id,
  $User.Id  =   Owner:User.Id
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Created By Assigned To Me</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>End_Date_Calc__c</fullName>
        <externalId>false</externalId>
        <formula>ActivityDate  &lt; TODAY()</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>End Date Calc</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Past_Due__c</fullName>
        <externalId>false</externalId>
        <formula>ActivityDate &lt; TODAY()  &amp;&amp;  TEXT(Status)&lt;&gt; &apos;Completed&apos;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Past Due</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Summary__c</fullName>
        <externalId>false</externalId>
        <label>Summary</label>
        <required>false</required>
        <type>TextArea</type>
    </fields>
